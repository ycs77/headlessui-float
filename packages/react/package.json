{
  "name": "@headlessui-float/react",
  "version": "0.9.1",
  "description": "Easily use Headless UI for React with Floating UI (Popper.js)",
  "license": "MIT",
  "homepage": "https://headlessui-float.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/react"
  },
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "react"
  ],
  "exports": {
    ".": {
      "require": "./dist/headlessui-float.cjs",
      "import": "./dist/headlessui-float.js"
    }
  },
  "main": "./dist/headlessui-float.cjs",
  "module": "./dist/headlessui-float.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.0.0",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "dependencies": {
    "@floating-ui/core": "^1.0.0",
    "@floating-ui/dom": "^1.0.0",
    "@floating-ui/react-dom": "^1.0.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.6.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "lodash.throttle": "^4.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "~4.7.0",
    "vite": "^3.0.0"
  }
}
