{
  "name": "@headlessui-float/react",
  "version": "0.10.1",
  "description": "Easily use Headless UI for React with Floating UI (Popper.js)",
  "license": "MIT",
  "homepage": "https://headlessui-float.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/react"
  },
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "react"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/headlessui-float.cjs",
      "import": "./dist/headlessui-float.mjs"
    }
  },
  "main": "./dist/headlessui-float.cjs",
  "module": "./dist/headlessui-float.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.app.json --noEmit && vite build && tsc -p tsconfig.app.json --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch",
    "test:unit": "vitest --root test/unit/",
    "test:functional": "playwright test test/functional",
    "test:functional:update": "yarn test:functional -u"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.0.0",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "dependencies": {
    "@floating-ui/core": "^1.0.0",
    "@floating-ui/dom": "^1.0.0",
    "@floating-ui/react-dom": "^1.0.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.4",
    "@playwright/test": "^1.30.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "jsdom": "^21.1.0",
    "lodash.throttle": "^4.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "~4.7.0",
    "vite": "^4.0.0",
    "vitest": "^0.28.5"
  }
}
