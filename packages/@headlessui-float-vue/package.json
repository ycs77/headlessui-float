{
  "name": "@headlessui-float/vue",
  "version": "0.7.0",
  "description": "Easily use Headless UI for Vue 3 with Floating UI (Popper.js)",
  "license": "MIT",
  "homepage": "https://headlessui-float.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/headlessui-float.git",
    "directory": "packages/@headlessui-float-vue"
  },
  "keywords": [
    "floating",
    "popper",
    "popover",
    "tooltip",
    "dropdown",
    "headless",
    "headlessui",
    "vue"
  ],
  "exports": {
    ".": {
      "require": "./dist/headlessui-float.cjs.js",
      "import": "./dist/headlessui-float.es.js"
    }
  },
  "main": "./dist/headlessui-float.cjs.js",
  "module": "./dist/headlessui-float.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly --outDir ./dist",
    "watch": "vite build --watch"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@floating-ui/core": "^1.0.0",
    "@floating-ui/dom": "^1.0.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "typescript": "~4.7.0",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^3.0.0",
    "vue": "^3.2.31",
    "vue-tsc": "^0.38.0"
  }
}
